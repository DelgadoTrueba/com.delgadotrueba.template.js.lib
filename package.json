{
  "name": "@delgadotrueba/template-ts",
  "version": "0.0.1",
  "description": "Description",
  "keywords": [
    "template-ts",
    "template-typescript"
  ],
  "license": "MIT",
  "author": {
    "name": "delgadotrueba",
    "email": "delgadotrueba@pobox.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/delgadotrueba/template-js"
  },
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run build:mjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.mjs.json",
    "lint": "eslint -c .eslintrc.json src/**/*.ts",
    "lint:fix": "pnpm run lint -- --fix",
    "test": "jest --projects jest.cjs.config.js jest.mjs.config.js --verbose",
    "test:cjs": "jest -c jest.cjs.config.js --verbose",
    "test:mjs": "jest -c jest.mjs.config.js --verbose",
    "test:coverage": "jest --projects jest.cjs.config.js jest.mjs.config.js --coverage",
    "prepare": "husky",
    "prepublishOnly": "pnpm run test && pnpm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/jest": "30.0.0",
    "@types/node": "24.5.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "commitlint": "19.8.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "jest-environment-jsdom": "30.1.2",
    "jest-environment-node": "30.1.2",
    "lint-staged": "16.1.6",
    "prettier": "3.3.3",
    "ts-jest": "29.4.4",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "tslib": "2.6.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/mjs/src/index.js",
  "types": "./dist/mjs/src/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/cjs/src/index.js",
        "types": "./dist/cjs/src/index.d.ts"
      },
      "import": {
        "default": "./dist/mjs/src/index.js",
        "types": "./dist/mjs/src/index.d.ts"
      }
    },
    "./public-api": {
      "require": {
        "default": "./dist/cjs/src/public-api.js",
        "types": "./dist/cjs/src/public-api.d.ts"
      },
      "import": {
        "default": "./dist/mjs/src/public-api.js",
        "types": "./dist/mjs/src/public-api.d.ts"
      }
    }
  },
  "sideEffects": false
}